  object OraQuery1: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO PDK_PERSONEL_RESIMLERI'
      '  (RESIM)'
      'VALUES'
      '  (EMPTY_BLOB())'
      'RETURNING'
      '  RESIM'
      'INTO'
      '  :RESIM')
    SQLDelete.Strings = (
      'DELETE FROM PDK_PERSONEL_RESIMLERI'
      'WHERE'
      '  ROWID = :Old_ROWID')
    SQLUpdate.Strings = (
      'UPDATE PDK_PERSONEL_RESIMLERI'
      'SET'
      '  RESIM=EMPTY_BLOB()'
      'WHERE'
      '  ROWID = :Old_ROWID'
      'RETURNING'
      '  RESIM'
      'INTO'
      '  :RESIM')
    SQLLock.Strings = (
      'SELECT * FROM PDK_PERSONEL_RESIMLERI'
      'WHERE'
      '  ROWID = :Old_ROWID'
      'FOR UPDATE NOWAIT')
    SQLRefresh.Strings = (
      'SELECT PDK_PERSONEL_RESIMLERI.RESIM FROM PDK_PERSONEL_RESIMLERI'
      'WHERE'
      '  ROWID = :Old_ROWID')
    SQL.Strings = (
      
        'SELECT r.rowid,(CASE WHEN R.RESIM IS NOT NULL THEN da_convert_jp' +
        'g(r.resim) END) RESIM,'
      '       ROWNUM SNO,'
      '       P.TC_KIMLIK_NO TC,'
      '       SYSTEM.buyukharfcevir(P.PERSONEL_ADI) || '#39' '#39' ||'
      '       SYSTEM.buyukharfcevir(P.SOYADI) "ADI SOYADI",'
      '       P.TC_KIMLIK_NO || '#39'.JPG'#39' "TC.JPG",'
      '     '
      ' (CASE'
      '   WHEN G.ACIKLAMA LIKE '#39'EBE%'#39' THEN'
      '    '#39'E'#39' || P.SICIL_NO'
      ' '
      '   WHEN G.ACIKLAMA LIKE '#39'HEM'#350#304'RE%'#39' THEN'
      '    '#39'H'#39' || P.SICIL_NO'
      ' '
      '   WHEN G.ACIKLAMA IN ('#39'B'#304'YOLOG'#39', '#39'D'#304'YET'#304'SYEN'#39') THEN'
      '    '#39'MM'#39' || P.SICIL_NO'
      ' '
      '   WHEN G.ACIKLAMA = '#39'ECZACI'#39' THEN'
      '    '#39'ECZ'#39' || P.SICIL_NO'
      ' '
      '   WHEN G.ACIKLAMA IN ('#39'ENFEKS'#304'YON HASTALIKLARI UZMANI'#39','
      '                       '#39'GENEL CERRAH'#304#39','
      '                       '#39'HASTANE Y'#214'NET'#304'C'#304'S'#304#39','
      '                       '#39'KADIN DO'#286'UM VE HASTALIKLARI UZ.'#39','
      '                       '#39'M'#304'KROB'#304'YOLOJ'#304' UZMANI'#39','
      '                       '#39'NEOTOLOJ'#304' UZMANI'#39','
      '                       '#39'PATOLOJ'#304' UZMANI'#39','
      '                       '#39'PRAT'#304'SYEN HEK'#304'M'#39','
      '                       '#39'RADYOLOJ'#304' UZMANI'#39','
      '                       '#39#199'OCUK CERRAH'#304'S'#304' UZMANI'#39','
      '                       '#39#199'OCUK HASTALIKLARI UZMANI'#39','
      '                       '#39'ANESTEZ'#304' UZMANI'#39','
      '                       '#39'G'#214'Z HASTALIKLARI UZMANI'#39','
      '                       '#39'Ba'#351'hekim Yard'#305'mc'#305's'#305#39','
      '                       '#39'BA'#350'HEK'#304'M'#39','
      '                       '#39'B'#304'YOK'#304'MYA UZMANI'#39','
      '                       '#39'Pratisyen Hekim'#39') THEN'
      '    '#39'DR'#39' || P.SICIL_NO'
      ' '
      '   WHEN G.ACIKLAMA = '#39'H'#304'ZMETL'#304#39' THEN'
      '    '#39'HIZ'#39' || P.SICIL_NO'
      ' '
      '   WHEN G.ACIKLAMA  IN'
      
        '        ('#39'LABORATUVAR TEKN'#304'SYEN'#304#39', '#39'R'#214'NTGEN TEKN'#304'SYEN'#304#39', '#39'SA'#286'LIK' +
        ' MEMURU'#39','#39'AC'#304'L TIP TEKN'#304'SYEN'#304#39','#39'Acil T'#305'p Teknikeri (Paramedik)'#39',' +
        #39'ANESTEZ'#304' TEKN'#304'KER'#304#39','
      '        '#39'ANESTEZ'#304' TEKN'#304'SYEN'#304#39') THEN'
      '    '#39'K'#39' || P.SICIL_NO'
      '   ELSE'
      '    P.SICIL_NO'
      ' END'
      ' '
      ' ) KUR_SIC_NO,'
      ' SYSTEM.BUYUKHARFCEVIR(G.ACIKLAMA) "GOREV UNVANI",'
      ' SYSTEM.BUYUKHARFCEVIR(IL.ADI) "NUFUSA KAYITLI OLDU'#286'U IL",'
      ' SYSTEM.BUYUKHARFCEVIR(ILCE.ADI) "NUFUSA KAYITLI OLDU'#286'U IL'#199'E",'
      ' P.CILTNO  CILT,'
      ' P.AILESIRA AILE_SIRA,'
      ' P.SIRANO SIRA_NO,'
      ' P.BABA_ADI BABAADI,'
      ' P.ANNE_ADI ANNEADI,'
      ' P.DOGUM_YERI DOGUMYERI,'
      ' P.DOGUM_TARIHI DOG_TAR,'
      ' Pk.Seri_No || '#39'-'#39' || Pk.Seri_No2 NUSUF_CUZ_SERI_NO,'
      
        ' translate(replace(replace(P.KAN_GRUBU, '#39'-'#39', '#39'(-)'#39'), '#39'+'#39', '#39'(+)'#39')' +
        ', '#39'H'#39', '#39'h'#39') KAN_GRB '
      
        ' from per_personel p,per_kimlik pk,skrsadresler IL,SKRSADRESLER ' +
        'ILCE,'
      '      PDK_PERSONEL_RESIMLERI R,PER_PERSONEL_SABIT G'
      ' where p.tc_kimlik_no=pk.tckimlik_no(+)'
      '       AND P.KAYITLI_IL=IL.ID(+)'
      '       AND P.KAYITLI_ILCE=ILCE.ID(+)'
      '       AND P.TC_KIMLIK_NO=R.TC_KIMLIK_NO(+)'
      '       AND P.GOREV_YERI=G.SIRA_NO(+)'
      '       AND G.TURU='#39'GOREV'#39
      '       AND NVL(P.ISTEN_AYRILDI,'#39'F'#39')<>'#39'T'#39
      '/*  FROM PER_PERSONEL       P'
      '  LEFT JOIN  PER_KIMLIK         PK'
      '   ON PK.TCKIMLIK_NO=P.TC_KIMLIK_NO'
